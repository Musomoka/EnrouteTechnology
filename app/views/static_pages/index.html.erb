	<script>
	$(document).ready(function(){
	$("a[href^='#']").click(function(e) {
	e.preventDefault();
	
	var position = $($(this).attr("href")).offset().top;

	$("body, html").animate({
		scrollTop: position
	},2000);
	});
}
	
	
	</script> 

